@import "styles/iconfont.wxss";

page,
view,
text,
swiper,
swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

page {
  /* 定义主题颜色 */
  --themeColor: #3366CC;
  /* 定义统一的字体大小 */
  font-size: 28rpx;
  height: 100%;
  background: #f4f5f9;
}

.space {
  width: 100%;
  height: 16rpx;
  background: #F4F5F9;
}
.line {
  width: 718px;
  height: 1px;
  background: #EEEEEE;
  margin-left: 32rpx;
}